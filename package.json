{
  "name": "wordcount",
  "displayName": "WordCount",
  "description": "Example VS Code Extension with Fable",
  "version": "0.1.0",
  "publisher": "me",
  "license": "Unlicense",
  "private": true,
  "main": "./out/extension",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "scripts": {
    "build": "dotnet fable src/ --run npx rollup -c",
    "build:watch": "rollup -c -w"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "rollup": "^2.38.2"
  }
}
